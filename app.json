{
  "expo": {
    "name": "Code Editor",
    "slug": "code-editor",
    "version": "1.0.0",
    "projectId": "code-editor-app",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "codeeditor",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codeeditor.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1E1E1E",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.codeeditor.app"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1E1E1E",
          "dark": {
            "backgroundColor": "#1E1E1E"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "29ef4024-0fb5-49be-a903-727447541f32"
      }
    },
    "owner": "kerrylaggon",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/29ef4024-0fb5-49be-a903-727447541f32"
    }
  }
}
